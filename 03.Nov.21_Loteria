// Sorteo, devuelve el último número de de la cifra

#include <stdio.h>

main()
{
	int num, cifra;
	
	printf ("NUMEROS DEL SORTEO ONCE");
	printf ("Introduzaca su numero completo: \n");
	scanf ("%d", &num);
	
	cifra=num%10;
	
	printf("El ultimo digito de su numero es: %d", cifra);
	
}
